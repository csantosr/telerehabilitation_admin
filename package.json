    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "mdue": "^0.1.2",
    "prettier": "^2.2.1",
    "vue": "^3.0.0",
    "vue-progress-path": "0.0.2",
    "vue-router": "^4.0.0-rc.1"
  },
